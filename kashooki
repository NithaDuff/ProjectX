         String ans[]=new String[6];
         ans[0]=jTextField26.getText();
         ans[1]=jTextField27.getText();
         ans[2]=jTextField28.getText();
         ans[3]=jTextField29.getText();
         ans[4]=jTextField30.getText();
         ans[5]=jTextField31.getText();
        input[0]=jTextField12.getText();
        input[1]=jTextField13.getText();
        input[2]=jTextField14.getText();
        input[3]=jTextField15.getText();
        input[4]=jTextField16.getText();
        input[5]=jTextField17.getText();
        int size=6;int score=0;
        RESE.setText("You got the following words wrong:");
       
        for(int i=0;i<size;i++)
            {
                if(input[i].equalsIgnoreCase(ans[i]))
                {
                    score++;
                }
                else
                {
                    
                    RESE.append("-"+"\n"+ans[i]);
                }
            }
            jTextField5.setText(score+"/"+size);

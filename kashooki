    private void jButton9ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        
         
         String input[]=new String[4];
         String ans[]=new String[4];
         ans[0]=jTextField11.getText();
         ans[1]=jTextField18.getText();
         ans[2]=jTextField19.getText();
         ans[3]=jTextField20.getText();
        input[0]=jTextField1.getText();
        input[1]=jTextField2.getText();
        input[2]=jTextField3.getText();
        input[3]=jTextField4.getText();
        int size=4;int score=0;
        RESE.setText("You got the following words wrong:");
       
        for(int i=0;i<size;i++)
            {
                if(input[i].equalsIgnoreCase(ans[i]))
                {
                    score++;
                }
                else
                {
                    
                    RESE.append("-"+"\n"+ans[i]);
                }
            }
            jTextField5.setText(score+"/"+size);
            if(score==size)
                RESE.setText("You got all the words right!!.CONGRATS!.:)");
            RES1.setVisible(true);
        

    }                                        


TIMER CLASS
package duffman;
public class time {

  public static void sleepbro100(String a) throws InterruptedException
    {
       for (int i = 0; i < a.length(); i++) 
        {
            char c=a.charAt(i);
            Thread.sleep(500);
            System.out.print(c);
         }              }}
